<!doctype html>
<html>
    <head>
        <title>What's on TV</title>
        <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
        <link href="/css/index.css" rel="stylesheet">
    </head>
    <body>
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="row">
                        <div class="col-lg-3 direction">
                            <h2>Today</h2>
                            <h4><%= theDate %></h4>
                            <a href="/daily" id="prev" class="btn btn-danger next-day">Previos</a>
                            <a href="/daily" id="next" class="btn btn-primary prev-day">Next</a>
                        </div>
                    </div>
                    <ul class="show-list">
                        <% for(var item in data){ %>
                            <li>
                                <div class="show">
                                    <h3><%= data[item].show.name %></h3>
                                    <img src="<%= data[item].show.image.medium %>" alt="">

                                </div>
                            </li>
                        <% } %>
                    </ul>
                </div>
            </div>
        </div>
        <script src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
        <script src="./config/js/test.js"></script>
    </body>
</html>